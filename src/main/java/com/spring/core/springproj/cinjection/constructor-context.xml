<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.spring.core.springproj.pojo.Person" name="p1">
	<constructor-arg name="name" value="Mrudula"></constructor-arg>
	<constructor-arg name="email" value="jasdhas@csdh.com"></constructor-arg>
	<constructor-arg name="personId" value="160"></constructor-arg>
</bean>
<bean class="com.spring.core.springproj.pojo.Person" name="p2" c:name="Manu" c:email="manu@gmai.com" c:personId="12345"></bean>
</beans>    