<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:p="http://www.springframework.org/schema/p"	
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.spring.core.springproj.pojo.Employee" name="emp1">
		<property name="name" value="Madhura Moogi"></property>
		<property name="phoneNumbers">
			<list>
				<value>9922739263</value>
				<value>8582321311</value>
			</list>
		</property>
		<property name="addresses">
		<set>
			<value>Pune, Maharashtra</value>
			<value>Bangalore, Karnataka</value>
			<value>Belgaum, Karnataka</value>
		</set>
		</property>
		<property name="courses">
			<map>
				<entry key="C1" value="Software Engineering"></entry>
				<entry key="C2" value="Algorithms and Data Structures"></entry>
			</map>
		</property>
	</bean>
    <!-- bean definitions here -->

</beans>